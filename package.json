{
  "name": "gatsby-typescript",
  "version": "2.0.0",
  "description": "Alternative typescript support plugin for Gatsbyjs. Aims to make using typescript in Gatsby as painless as possible",
  "main": "index.js",
  "repository": "https://github.com/d4rekanguok/gatsby-plugin-ts.git",
  "author": "Derek Nguyen <derek@penandpillow.com>",
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "build": "lerna run build",
    "test": "jest"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.2",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "lerna": "^3.16.4",
    "prettier": "^1.19.1"
  }
}
